                        <!--tools:ignore="ContentDescription"/>-->

                    <!--<TextView-->
                        <!--android:id="@+id/h5_bt_dot_number1"-->
                        <!--android:layout_width="wrap_content"-->
                        <!--android:layout_height="wrap_content"-->
                        <!--android:background="@drawable/h5_point_large"-->
